<script type="text/javascript">
    $(document).ready(function () {
        $(function () {
            $("#dialog").dialog({

                autoOpen: false,
                modal: true,
                show: {
                    effect: "blind",
                    duration: 1000
                },
                hide: {
                    effect: "fold",
                    duration: 1000
                }

            });
        });
    }

    );
    $("#opener").click(function () {
        $("#dialog").dialog("open");
    });
    $("#opener").click(function () {
        var state = true;
        if (state) {
            $("#dialog").animate({
                backgroundColor: "white",
                color: "black"

            }, 1000);
        }
    });

</script>

