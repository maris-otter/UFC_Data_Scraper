            $("#dialog").dialog({

                autoOpen: false,
                modal: true,
                show: {
                    effect: "blind",
                    duration: 1000
                },
                hide: {
                    effect: "fold",
                    duration: 1000
                }

            });
        });
    }

    );
    $("#opener").click(function () {
        $("#dialog").dialog("open");
    });
